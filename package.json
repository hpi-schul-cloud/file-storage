    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.2",
    "@nestjs/swagger": "^11.2.0",
    "axios": "^1.9.0",
    "busboy": "^1.6.0",
    "clamscan": "^2.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie": "^1.0.2",
    "file-type": "^21.0.0",
    "gm": "^1.25.1",
    "html-entities": "^2.6.0",
    "jsonwebtoken": "^9.0.2",
    "load-esm": "^1.0.2",
    "lodash": "^4.17.21",
    "nest-winston": "^1.10.2",
    "passport-headerapikey": "^1.2.2",
    "passport-jwt": "^4.0.1",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "response-time": "^2.3.3",
    "rxjs": "^7.8.2",
    "sanitize-html": "^2.17.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.27.0",
    "@jest/globals": "^29.7.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.2",
    "@openapitools/openapi-generator-cli": "^2.20.2",
