{
  "name": "file-storage",
  "version": "0.0.1",
  "description": "Filestorage for svs",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hpi-schul-cloud/file-storage.git"
  },
  "author": "Schulcloud-Verbund",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/hpi-schul-cloud/file-storage/issues"
  },
  "homepage": "https://github.com/hpi-schul-cloud/file-storage#readme",
  "engines": {
    "node": "22",
    "npm": ">=9"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start:files-storage": "nest start files-storage & nest start files-storage-amqp & nest start preview-generator-amqp",
    "start:files-storage:dev": "nest start files-storage --watch & nest start files-storage-amqp --watch & nest start preview-generator-amqp --watch",
    "start:files-storage:debug": "nest start files-storage --debug --watch & nest start files-storage-amqp --debug --watch  & nest start preview-generator-amqp --debug --watch",
    "start:files-storage:prod": "node dist/apps/files-storage.app",
    "start:files-storage-amqp:prod": "node dist/apps/files-storage-consumer.app",
    "start:preview-generator-amqp:prod": "node dist/apps/preview-generator-consumer.app",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "pregenerate-client:authorization-api": "rimraf ./src/infra/authorization-client/authorization-api-client",
    "generate-client:authorization-api": "openapi-generator-cli generate -c ./openapitools.json --generator-key authorization-api"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.617.0",
